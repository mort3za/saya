{
  "name": "saya",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-modern": "vue-cli-service build --modern",
    "build-report": "vue-cli-service build --report",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx,vue,json}' ",
    "fix-style": "prettier-stylelint --write 'src/**/*.{css,scss}' ",
    "swagger": "mkdir -p temp && ts-node src/connection/swagger-axios.ts && tsc -p ./temp/resources.ts --moduleResolution classic --target es6 --outDir ./src/connection/",
    "pwa-asset-generator-icon": "pwa-asset-generator public/img/logo-512.png public/img/icons/ --background '#ea5455' --manifest public/manifest.json --single-quotes --index public/index.html --scrape false --path '<%= BASE_URL %>' --icon-only",
    "pwa-asset-generator-splash": "pwa-asset-generator public/img/splash-screen.png public/img/icons/ --background '#ea5455' --single-quotes --index public/index.html --scrape false --path '<%= BASE_URL %>' --splash-only"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "axios": "0.19.2",
    "bootstrap": "4.4.1",
    "core-js": "3.6.4",
    "date-fns": "1.30.1",
    "lodash-es": "4.17.15",
    "marked": "0.8.0",
    "material-design-icons-iconfont": "5.0.1",
    "portal-vue": "^2.1.7",
    "register-service-worker": "1.6.2",
    "validator": "12.2.0",
    "vue": "2.6.11",
    "vue-fragment": "^1.5.1",
    "vue-meta": "^2.3.3",
    "vue-router": "3.1.5",
    "vuetify": "2.2.15",
    "vuex": "3.1.2",
    "vuex-persist": "2.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "4.17.3",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-pwa": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-typescript": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "1.0.0-beta.31",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "esm": "3.2.25",
    "postcss-easing-gradients": "3.0.1",
    "prettier": "^1.19.1",
    "pwa-asset-generator": "^2.2.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "sass-rem": "2.0.1",
    "swagger-axios-codegen": "0.9.17",
    "ts-node": "8.6.2",
    "typescript": "~3.7.5",
    "vue-cli-plugin-vuetify": "2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "1.4.3"
  }
}
